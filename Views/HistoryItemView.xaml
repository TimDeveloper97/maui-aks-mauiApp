<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="VSmauiApp.Views.HistoryItemView">
    <Frame Margin="8,2">
        <Grid ColumnDefinitions="7*,3*">
            <VerticalStackLayout>
                <Label Text="{Binding Path=t}" TextColor="Green" FontSize="9" />
                <Label Text="{Binding Path=s}" />
            </VerticalStackLayout>
        </Grid>
    </Frame>
</ContentView>